

<ManagerProfile>:
    ScrollView:
        orientation:'vertical'
        MDBoxLayout:
            size_hint_y:None
            height:self.minimum_height
            orientation:'vertical'
            MDBoxLayout:
                size_hint_y:None
                height:'60dp'
                md_bg_color:0.13, 0.53, 0.95,1
                MDIconButton:
                    icon:'arrow-left-bold-outline'
                    color:1,1,1,1
                    on_release:app.root.current = 'homepage'
                MDLabel:
                    size_hint_x:None
                    width:'200dp'
                    color:1,1,1,1
                    font_size:'25dp'
                    bold:True
                    halign:'center'
                    text:'Crie seu perfil'
                MDIconButton:
                    icon:'refresh'
                    on_release:root.infill()

            Widget:
                size_hint_y:None
                height:'10dp'
            MDCard:
                orientation:'vertical'
                size_hint:.8,None
                height:'200dp'
                radius:12,12,12,12
                pos_hint:{'center_x':.5}
                MDBoxLayout:
                    orientation:'vertical'
                    size_hint_y:None
                    height:'60dp'
                    pos_hint:{'center_x':.5,'center_y':.8}
                    spacing:'4dp'
                    MDLabel:
                        size_hint_y:None
                        height:'30dp'
                        pos_hint:{'center_x':.5}
                        bold:True
                        color:1,.5,.5,1
                        font_size:'30dp'
                        id:text_name
                MDBoxLayout:
                    orientation:'vertical'
                    size_hint_y:None
                    height:'60dp'
                    spacing:'11dp'
                    pos_hint:{'center_x':.5,'center_y':.67}
                    MDBoxLayout:
                        MDLabel:
                            font_size:'9dp'
                            text:'Entrada do serviço'
                        MDLabel:
                            font_size:'9dp'
                            text:'Horas'
                            halign:'center'
                        MDLabel:
                            font_size:'9dp'
                            halign:'right'
                            text:'saida do serviço'
                    MDBoxLayout:
                        MDTextFieldRect:
                            size_hint_x:None
                            width:'50dp'
                            hint_text:'00:00'
                            on_focus:root.validate('entry')
                            id:entry
                        MDBoxLayout:
                            size_hint_y:None
                            height:'50dp'
                            pos_hint:{'center_y':.5}
                            ScrollView:
                                MDBoxLayout:
                                    orientation:'vertical'
                                    size_hint_y:None
                                    height:self.minimum_height
                                    id:hours
                        MDTextFieldRect:
                            size_hint_x:None
                            width:'50dp'
    #                        pos_hint:{'center_x':.9}
                            hint_text:'00:00'
                            on_focus:root.validate('exit')
                            id:exit
                Widget:
                    size_hint_y:None
                    height:'10dp'

                MDBoxLayout:
                    orientation:'vertical'
                    size_hint_y:None
                    height:'50dp'
                    Label:
                        font_size:'9dp'
                        text:'Informe o tempo entre cada agenda'
                        color:0,0,0,1
                    MDTextFieldRect:
                        size_hint_x:None
                        width:'50dp'
                        pos_hint:{'center_x':.5}
                        hint_text:'00:00'
                        id:space_temp
                Widget:
                    size_hint_y:None
                    height:'10dp'

                MDBoxLayout:
                    size_hint_y:None
                    height:'30dp'
                    pos_hint:{'center_x':.5}
                    MyButtonCard:
                        first_text:'Salvar'
                        secound_text:'Horas'
                        on_release:app.root.get_screen('managerprofile').save_data()

            Widget:
                size_hint_y:None
                height:'30dp'
            MDBoxLayout:
                orientation:'vertical'
                size_hint:.7,None
                height:'270dp'
                spacing:'10dp'
                pos_hint:{'center_x':.5,'center_y':.3}
                MDLabel:
                    size_hint_y:None
                    height:'30dp'
                    halign:'center'
                    bold:True
                    font_size:'15dp'
                    markup:True
                    text:'Adicione seus tipo de serviços\n[size=18]Adicione quanto serviços for nessesario[/size]'
                MDBoxLayout:
                    orientation:'vertical'
                    size_hint_y:None
                    height:'100dp'
                    spacing:'10dp'#
                    MDTextField:
                        size_hint_y:None
                        height:'30dp'
                        mode:'rectangle'
                        hint_text:'Nome do serviço'
                        id:nome_servico
                    MDBoxLayout:
                        spacing:'20dp'
                        MDBoxLayout:
                            orientation:'vertical'
                            spacing:'4dp'
                            MDLabel:
                                text:'Tempo'
                                bold:True
                                font_size:'10dp'
                            TextInput:
                                size_hint_y:None
                                height:'30dp'
                                hint_text:'00:00'
                                id:tempo
                        MDBoxLayout:
                            orientation:'vertical'
                            spacing:'4dp'
                            MDLabel:
                                bold:True
                                font_size:'10dp'
                                text:'Valor:'
                            TextInput:
                                size_hint_y:None
                                height:'30dp'
                                hint_text:'R$:'
                                id:valor
                MyButtonCard:
                    first_text:'Adicionar'
                    secound_text:'serviço'
                    on_release:app.root.get_screen('managerprofile').save_servicos()

                MDCard:
                    md_bg_color:0.78, 0.80, 0.83,1
                    radius:8,8,8,8
                    MDCard:
                        orientation:'vertical'
                        size_hint:None,None
                        width:self.parent.width - 10
                        height:self.parent.height - 10
                        radius:8,8,8,8
                        halign:'center'
                        pos_hint:{'center_x':.5,'center_y':.5}
                        MDBoxLayout:
                            size_hint_y:None
                            height:'10dp'
                            MDLabel:
                                size_hint_x:.40
                                font_size:'10dp'
                                bold:True
                                color:0,0,0,.5
                                text:'Serviços'
                            MDLabel:
                                size_hint_x:.40
                                font_size:'10dp'
                                bold:True
                                color:0,0,0,.5
                                text:'Tempo'
                            MDLabel:
                                size_hint_x:.20
                                font_size:'10dp'
                                bold:True
                                color:0,0,0,.5
                                text:'ValorR$'
                        MDBoxLayout:
                            ScrollView:
                                MDBoxLayout:
                                    orientation:'vertical'
                                    size_hint_y:None
                                    height:self.minimum_height
                                    id:categorie

            MDBoxLayout:
                size_hint:.8,None
                height:'30dp'
                pos_hint:{'center_x':.5}
                canvas:
                    Color:
                        rgba:0,1,1,1
                    Rectangle:
                        size:self.width,3
                        pos:self.pos
