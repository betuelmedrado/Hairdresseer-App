                Widget:
            MDCard:
                size_hint:.7,None
                height:'100dp'
                pos_hint:{'center_x':.5}
                md_bg_color:0.78, 0.80, 0.83,1
                radius:8,8,8,8
                MDCard:
                    orientation:'vertical'
                    size_hint:None,None
                    width:self.parent.width - 10
                    height:self.parent.height - 10
                    radius:8,8,8,8
                    halign:'center'
                    pos_hint:{'center_x':.5,'center_y':.5}
                    MDBoxLayout:
                        size_hint_y:None
                        height:'10dp'
                        MDLabel:
                            size_hint_x:.40
                            font_size:'10dp'
                            bold:True
                            color:1,0,0,1
                            text:'Nome'
                    MDBoxLayout:
                        ScrollView:
                            MDBoxLayout:
                                orientation:'vertical'
                                size_hint_y:None
                                height:self.minimum_height
                                id:box_socio

<Popup_widgets>:
    orientation:'vertical'
    id_work:''
    servico:''
    tempo:''
    valor:''
    spacing:'10dp'
    padding:'10dp'
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            size_hint_y:None
            height:'15dp'
            text:'Serviço'
            bold:True
            color:0.39, 0.40, 0.42,1
        TextInput:
            size_hint_y:None
            height:'30dp'
            text:root.servico
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            size_hint_y:None
            height:'15dp'
            text:'Tempo'
            bold:True
            color:0.39, 0.40, 0.42,1
        TextInput:
            size_hint_y:None
            height:'30dp'
            text:root.tempo
            id:id_tempo
    MDBoxLayout:
        orientation:'vertical'
        MDLabel:
            size_hint_y:None
            height:'15dp'
            text:'Valor: R$'
            bold:True
            color:0.39, 0.40, 0.42,1
        TextInput:
            size_hint_y:None
            height:'30dp'
            text:root.valor
            id:id_valor
    MDBoxLayout:
        spacing:'15dp'
        id:box_button
        MDRaisedButton:
            text:'Editar'
            on_release:root.pop_edit(root.id_work)

        MDRaisedButton:
            text:'Excluir'
            on_release:root.pop_delet_work(root.id_work)

        MDTextButton:
            text:'Sair'
            on_release:app.root.get_screen('managerprofile').dismiss_popup()