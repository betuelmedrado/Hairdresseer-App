
# The table of hours
<Table_shedule>:
    id_button:''
    id_schedule:''
    hours:''
    hours_second:''
    time:''
    client:''
    size_hint_y:None
    height:'100dp'

    MDCard:
        orientation:'vertical'
        ripple_behavior:True
        md_bg_color:1,1,1,.3
        radius:7,7,7,7
        id:card
        on_release: app.root.get_screen('viewshedule').inf_schedule_client(root.id_button, root.id_schedule, root.hours, root.hours_second, root.time, root.client)
        MDBoxLayout:
            MDBoxLayout:
                orientation:'vertical'
                size_hint_x:None
                width:'50dp'
                canvas.before:
                    Color:
                        rgba:1,1,1,.7
                    Rectangle:
                        size:root.width , root.height
                        pos:root.pos
#                        source:'images/moldura_verde.png'
                        source:'images/fundo_madeira.jpg'
                Label:
                    text:root.hours
                    outline_color:.2,1,1,1
                    outline_width:'5dp'
                    bold:True
                    color:0,0,0,1
                Label:
                    outline_color:.2,1,1,1
                    outline_width:'5dp'
                    bold:True
                    color:0,0,0,1
                    text:root.hours_second
            FloatLayout:
                MDLabel:
                    font_size:'25dp'
                    bold:True
                    halign:'center'
                    color:0,0,0,1
                    outline_width:'2dp'
                    outline_color:1,1,1,1
                    pos_hint:{'center_x':.3,'center_y':.8}
                    text:root.client
                Label:
                    font_size:'10dp'
                    color:0,0,0,.7
                    bold:True
                    pos_hint:{'center_x':.9,'center_y':.9}
                    text:root.time
        MDLabel:
            size_hint_y:None
            height:'13dp'
            font_size:'8dp'
            color:0,0,0,1
            halign:'center'
            markup:True
            text:'[color=#6B0A00][b]"Atenção":[/b][/color] Você tem 15 minutos para desmarcar apos agendamento!'

