
<BoxBlocked>:
    size_hint_y:None
    height:'110dp'
    radius:8,8,8,8
    ripple_behavior:True
    nome:''
    quant_cancel:''
    cpf:''
    tel:''
    id_client:''
    on_release:root.pop_dis_block(self,self.nome, self.id_client)
    MDCard:
        size_hint_x:None
        width:'10dp'
        radius:8,0,0,8
        id:color_card
    MDBoxLayout:
        orientation:'vertical'
        padding:('8dp',0,'5dp',0)
        MDBoxLayout:
            size_hint_y:None
            height:'25dp'
            Label:
                size_hint_x:None
                width:'50dp'
                font_size:'11dp'
                text:'Nome:'
                bold:True
                color:0,0,0,.4
            MDLabel:
                text:root.nome
        MDSeparator:

        MDBoxLayout:
            orientation:'vertical'
            padding:(0,'5dp',0,0)
            MDBoxLayout:
                size_hint_y:None
                height:'20dp'
                MDIcon:
                    size_hint_x:None
                    width:'23dp'
                    icon:'badge-account-outline'
                MDLabel:
                    font_size:'13dp'
                    bold:True
                    text:'Cpf:'
                    color:0,0,0,.4
                MDIcon:
                    size_hint_x:None
                    width:'23dp'
                    icon:'phone'
                MDLabel:
                    font_size:'13dp'
                    bold:True
                    text:'Phone:'
                    color:0,0,0,.4
            MDBoxLayout:
                size_hint_y:None
                height:'23dp'
                MDLabel:
                    text:root.cpf
                MDLabel:
                    text:root.tel
            MDCard:
                orientation:'vertical'
                radius:8,8,8,8
                md_bg_color:0,0,.5,.1
                Label:
                    color:0,0,0,.8
                    text:'Quantidade cancelada!'
                Label:
                    bold:True
                    text:root.quant_cancel
                    outline_width:3
                    outline_color:0,0,0,1
                    id:text_quant
