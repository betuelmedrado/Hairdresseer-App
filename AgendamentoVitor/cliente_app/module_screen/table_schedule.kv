
# The table of hours
<Table_shedule>:
    id_button:''
    id_schedule:''
    hours:''
    hours_2:''
    client:''
    time_cancel:''
    size_hint_y:None
    height:'100dp'

    MDCard:
        orientation:'vertical'
        ripple_behavior:True
        md_bg_color:1,1,1,.3
        radius:7,7,7,7
        id:card
#        on_release: app.root.get_screen('viewshedule').get_hours(root.id_button, root.hours)
        on_release: app.root.get_screen('viewshedule').if_blocked(root.id_button, root.hours)
        MDBoxLayout:
            MDBoxLayout:
                orientation:'vertical'
                size_hint_x:None
                width:'40dp'
                canvas.before:
                    Color:
                        rgba:1,1,1,.7
                    Rectangle:
                        size:root.width , root.height
                        pos:root.pos
                        source:'images/moldura_verde.png'
                Label:
                    text:root.hours
                    outline_color:.2,1,1,1
                    outline_width:'5dp'
                    bold:True
                    color:0,0,0,1
                Label:
                    outline_color:.2,1,1,1
                    outline_width:'5dp'
                    bold:True
                    color:0,0,0,1
                    text:root.hours_2
            MDLabel:
                text:root.client
                bold:True
                halign:'center'
                color:0,0,0,1
        MDLabel:
            size_hint_y:None
            height:'13dp'
            font_size:'15sp'
            color:0,0,0,1
            halign:'center'
            markup:True
            text:root.time_cancel

