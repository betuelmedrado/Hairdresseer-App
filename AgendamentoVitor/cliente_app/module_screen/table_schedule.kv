<Table_block>:
    id_button:''
    id_schedule:''
    hours:''
    hours_2:''
    client:''
    size_hint_y:None
    height:'100dp'
    MDCard:
        ripple_behavior:True
        md_bg_color:.0,0,0,.5
        radius:5,5,5,5
        id:card
#        on_release: app.root.get_screen('viewshedule').get_hours(root.id_button, root.hours)
        MDBoxLayout:
            orientation:'vertical'
            size_hint_x:None
            width:'70dp'
            canvas:
                Color:
                    rgba:1,1,1,.3
                Rectangle:
                    size:root.width , root.height
                    pos:root.pos
                    source:'images/fundo_madeira.jpg'
            MDLabel:
                text:root.hours
                outline_color:.2,1,1,1
                outline_width:'5dp'
                bold:True
                color:0,0,0,1
            MDLabel:
                color:0,0,0,1
                outline_color:.2,1,1,1
                outline_width:'5dp'
                bold:True
                text:root.hours_2
        FloatLayout:
            MDLabel:
                font_size:'30dp'
                text:root.client
                outline_width:1
                outline_color:1,0,0,1
                bold:True
                color:1,1,1,.8
                pos_hint:{'center_x':.6, 'center_y':.5}
