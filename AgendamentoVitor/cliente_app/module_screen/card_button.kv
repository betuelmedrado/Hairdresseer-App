
# The class of Manager hairdresser
<MyCardButton>:
    size_hint:None,None
    width:'200dp'
    height:'80dp'
    pos_hint:{'center_x':.5,'center_y':.5}
    md_bg_color:0,1,1,1
    ripple_behavior:True
    radius:5,5,5,5
    socio_or_manager:''
    id_user:''
    nome:''
    on_release:root.send_info(self.socio_or_manager, self.id_user)
    Image:
        source:'images/person.png'
    MDLabel:
        color:0,0,0,1
        bold:True
        font_size:'25dp'
        text:root.nome
    MDBoxLayout:
        size_hint_x:None
        width:'30dp'
        pos_hint:{'center_y':.9}
        id:image_check
